package com.better517na.JavaServiceRouteHelper.model;

import java.io.Serializable;
import java.util.Date;

/**
 * 可用路由列表缓存.
 * 
 * @author yishao
 *
 */
public class ServiceRouteInfoCache implements Serializable {

    /**
     * 序列化.
     */
    private static final long serialVersionUID = 1L;

    /**
     * 接口全名.
     */
    private String contractName;

    /**
     * 程序唯一编号.
     */
    private String uniqueSign;

    /**
     * 目标服务绑定类型.
     */
    private String bindingType;

    /**
     * 目标服务IP地址.
     */
    private String serviceIP;

    /**
     * 目标服务端口.
     */
    private int servicePort;

    /**
     * . 服务类型
     */
    private String serviceType;

    /**
     * 服务名+客户端访问服务端的地址.
     */
    private String svcPath;

    /**
     * 当前时间(缓存).
     */
    private Date currentTime;

    /**
     * 设置contractName.
     * 
     * @return 返回contractName
     */
    public String getContractName() {
        return contractName;
    }

    /**
     * 获取contractName.
     * 
     * @param contractName
     *            要设置的contractName
     */
    public void setContractName(String contractName) {
        this.contractName = contractName;
    }

    /**
     * 设置uniqueSign.
     * 
     * @return 返回uniqueSign
     */
    public String getUniqueSign() {
        return uniqueSign;
    }

    /**
     * 获取uniqueSign.
     * 
     * @param uniqueSign
     *            要设置的uniqueSign
     */
    public void setUniqueSign(String uniqueSign) {
        this.uniqueSign = uniqueSign;
    }

    /**
     * 设置bindingType.
     * 
     * @return 返回bindingType
     */
    public String getBindingType() {
        return bindingType;
    }

    /**
     * 获取bindingType.
     * 
     * @param bindingType
     *            要设置的bindingType
     */
    public void setBindingType(String bindingType) {
        this.bindingType = bindingType;
    }

    /**
     * 设置serviceIP.
     * 
     * @return 返回serviceIP
     */
    public String getServiceIP() {
        return serviceIP;
    }

    /**
     * 获取serviceIP.
     * 
     * @param serviceIP
     *            要设置的serviceIP
     */
    public void setServiceIP(String serviceIP) {
        this.serviceIP = serviceIP;
    }

    /**
     * 设置servicePort.
     * 
     * @return 返回servicePort
     */
    public int getServicePort() {
        return servicePort;
    }

    /**
     * 获取servicePort.
     * 
     * @param servicePort
     *            要设置的servicePort
     */
    public void setServicePort(int servicePort) {
        this.servicePort = servicePort;
    }

    /**
     * 设置svcPath.
     * 
     * @return 返回svcPath
     */
    public String getSvcPath() {
        return svcPath;
    }

    /**
     * 获取svcPath.
     * 
     * @param svcPath
     *            要设置的svcPath
     */
    public void setSvcPath(String svcPath) {
        this.svcPath = svcPath;
    }

    /**
     * 设置currentTime.
     * 
     * @return 返回currentTime
     */
    public Date getCurrentTime() {
        return currentTime;
    }

    /**
     * 获取currentTime.
     * 
     * @param currentTime
     *            要设置的currentTime
     */
    public void setCurrentTime(Date currentTime) {
        this.currentTime = currentTime;
    }

    /**
     * 构造函数.
     * 
     * @param contractName
     *            contractName
     * @param uniqueSign
     *            uniqueSign
     * @param bindingType
     *            bindingType
     * @param serviceIP
     *            serviceIP
     * @param servicePort
     *            servicePort
     * @param svcPath
     *            svcPath
     * @param serviceType
     *            serviceType
     */
    public ServiceRouteInfoCache(String contractName, String uniqueSign, String bindingType, String serviceIP, int servicePort, String svcPath, String serviceType) {
        this.contractName = contractName;
        this.uniqueSign = uniqueSign;
        this.bindingType = bindingType;
        this.serviceIP = serviceIP;
        this.servicePort = servicePort;
        this.svcPath = svcPath;
        this.serviceType = serviceType;
    }

    /**
     * {@inheritDoc}.
     */
    @Override
    public String toString() {
        return "ServiceRouteInfoCache [contractName=" + contractName + ", uniqueSign=" + uniqueSign + ", bindingType=" + bindingType + ", serviceIP=" + serviceIP + ", servicePort=" + servicePort
                + ", svcPath=" + svcPath + ", currentTime=" + currentTime + ", serviceType=" + serviceType + "]";
    }

    /**
     * 构造函数.
     * 
     */
    public ServiceRouteInfoCache() {
    }

    /**
     * 设置serviceType.
     * 
     * @return 返回serviceType
     */
    public String getServiceType() {
        return serviceType;
    }

    /**
     * 获取serviceType.
     * 
     * @param serviceType
     *            要设置的serviceType
     */
    public void setServiceType(String serviceType) {
        this.serviceType = serviceType;
    }
}
