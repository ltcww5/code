<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="javasrehcahe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd" updateCheck="false">
    <diskStore path="java.io.tmpdir" />

    <defaultCache eternal="false"
        timeToIdleSeconds="130" timeToLiveSeconds="130" overflowToDisk="false"
        maxEntriesLocalHeap="10000"  memoryStoreEvictionPolicy="LFU" />

    <cache name="ServiceRouteInfoCache" eternal="false"
        overflowToDisk="false" timeToIdleSeconds="130" timeToLiveSeconds="130"
        maxEntriesLocalHeap="10000" memoryStoreEvictionPolicy="LFU" />

    <cache name="ServiceDowntimeInfoCache" eternal="false"
        overflowToDisk="false" timeToIdleSeconds="130" timeToLiveSeconds="130"
        maxEntriesLocalHeap="10000" memoryStoreEvictionPolicy="LFU" />
</ehcache>
